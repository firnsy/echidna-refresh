    <script type="text/ng-template" id="/events.html">
      <div class="container-fluid">
        <div class="row-fluid">
          <div class="span12 center" style="text-align: center;">
            <div class="widget-box">
              <div class="widget-title">
                <span class="icon"><i class="icon-exclamation-sign"></i></span>
                <h5>Events</h5><h6 class="muted">({{eventCount()}} unclassified records)</h6>
                <span class="paginate-page-size pull-right">
                  <p>Show<span><select ng-model="pageSize" ng-options="p.name for p in pageSizes" ng-change="search()"></select></span> per page</p>
                </span>
              </div>
              <div class="widget-content nopadding">
                <table class="table table-clean">
                  <thead>
                    <tr>
                      <th ng-click="sortBy('sig_priority')" ng-class="getSortClass('sig_priority')">Priority</th>
                      <th ng-click="sortBy('net_src_ip')" ng-class="getSortClass('net_src_ip')">Source IP</th>
                      <th ng-click="sortBy('net_dst_ip')" ng-class="getSortClass('net_dst_ip')">Desination IP</th>
                      <th ng-click="sortBy('sig_message')" ng-class="getSortClass('sig_message')">Signature</th>
                      <th ng-click="sortBy('timestamp')" ng-class="getSortClass('timestamp')">Timestamp</th>
                    </tr>
                  </thead>
                  <tbody ng-repeat="e in pagedItems[currentPage] | orderBy:sortField:sortReverse">
                    <tr ng-click="toggleEventDetails(e)">
                      <td><span class="badge badge-priority" ng-class="priorityClass(e.sig_priority)">{{e.sig_priority}}</span></td>
                      <td>{{formatEventSourceIP(e)}}</td>
                      <td>{{formatEventDestinationIP(e)}}</td>
                      <td>{{e.sig_message}}</td>
                      <td>{{formatEventTimestamp(e)}}</td>
                    </tr>
                    <tr ng-show="showEventDetails(e)">
                      <td colspan="5">
                        <div class="row-fluid">
                          <div class="span4">
                            <label class="checkbox">
                              <input type="checkbox"> Show Rule
                            </label>
                          </div>
                        </div>
                        <div class="row-fluid">
                          <div class="span4">
                            <h4>Some Info</h4>
                          </div>
                          <div class="span4">
                            <h5>Some Othe5</h5>
                          </div>
                          <div class="span4">
                            <h4>Some Other</h4>
                          </div>
                        </div>
                        <div class="row-fluid">
                          <div class="span12">
                            <hexdump></hexdump
                          </div>
                        </div>
                      </td>
                    </tr>
                  </tbody>
                </table>
                <div class="widget-footer fg-toolbar">
                  <div class="input-prepend pull-left">
                    <span class="add-on"><i class="icon-search"></i></span>
                    <input type="text" ng-model="query" ng-change="search()" placeholder="search"></input>
                  </div>
                  <div class="pull-right">
                    <div class="pagination pagination-clean">
                      <ul>
                        <li ng-class="{disabled: isFirstPage()}"><a ng-click="firstPage()">&laquo;</a></li>
                        <li ng-class="{disabled: isFirstPage()}"><a ng-click="prevPage()">&lsaquo;</a></li>
                        <li ng-repeat="n in getPageList()" ng-class="{active: currentPage == n}"><a ng-click="setPage(n)">{{n+1}}</a></li>
                        <li ng-class="{disabled: isLastPage()}"><a ng-click="nextPage()">&rsaquo;</a></li>
                        <li ng-class="{disabled: isLastPage()}"><a ng-click="lastPage()">&raquo;</a></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row-fluid">
          <div id="footer" class="span12">&mdash; <a href="https://github.com/firnsy/echidna-refresh">echidna</a> &ndash; v0.1 (alpha) &mdash;</div>
        </div>
      </div>
    </script>

    <script type="text/ng-template" id="/sessions.html">
      <div class="container-fluid">
        <div class="row-fluid">
          <div class="span12 center" style="text-align: center;">
            <div class="widget-box">
              <div class="widget-title">
                <span class="icon"><i class="icon-exclamation-sign"></i></span>
                <h5>Sessions</h5><h6 class="muted">({{sessionCount()}} unclassified records)</h6>
                <span class="paginate-page-size pull-right">
                  <p>Show<span><select ng-model="pageSize" ng-options="p.name for p in pageSizes" ng-change="search()"></select></span> per page</p>
                </span>
              </div>
              <div class="widget-content nopadding">
                <table class="table table-clean">
                  <thead>
                    <tr>
                      <th ng-click="sortBy('time_start')" ng-class="getSortClass('time_start')">Start Time</th>
                      <th ng-click="sortBy('time_end')"   ng-class="getSortClass('time_end')">End Time</th>
                      <th ng-click="sortBy('net_src_ip')" ng-class="getSortClass('net_src_ip')">Source</th>
                      <th ng-click="sortBy('net_dst_ip')" ng-class="getSortClass('net_dst_ip')">Destination</th>
                      <th ng-click="sortBy('net_protocol')" ng-class="getSortClass('net_protocol')">Protocol</th>
                    </tr>
                  </thead>
                  <!--<tbody ng-repeat="session in pagedItems[currentPage] | orderBy:sortField:sortReverse">-->
                  <tbody ng-repeat="session in pagedItems[currentPage] | orderBy:sortField:sortReverse">
                    <tr ng-click="toggleSessionDetails(session)">
                      <td><span class="" ng-class="priorityClass(session.time_start)">{{session.time_start}}</span></td>
                      <td>{{session.time_end}}</td>
                      <td>{{formatSessionSrc(session)}}</td>
                      <td>{{formatSessionDst(session)}}</td>
                      <td>{{formatProtocol(session)}}</td>
                    </tr>
                  </tbody>
                </table>
                <div class="widget-footer fg-toolbar">
                  <div class="input-prepend pull-left">
                    <span class="add-on"><i class="icon-search"></i></span>
                    <input type="text" ng-model="query" ng-change="search()" placeholder="search"></input>
                  </div>
                  <div class="pull-right">
                    <div class="pagination pagination-clean">
                      <ul>
                        <li ng-class="{disabled: isFirstPage()}"><a ng-click="firstPage()">&laquo;</a></li>
                        <li ng-class="{disabled: isFirstPage()}"><a ng-click="prevPage()">&lsaquo;</a></li>
                        <li ng-repeat="n in getPageList()" ng-class="{active: currentPage == n}"><a ng-click="setPage(n)">{{n+1}}</a></li>
                        <li ng-class="{disabled: isLastPage()}"><a ng-click="nextPage()">&rsaquo;</a></li>
                        <li ng-class="{disabled: isLastPage()}"><a ng-click="lastPage()">&raquo;</a></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row-fluid">
          <div id="footer" class="span12">&mdash; <a href="https://github.com/firnsy/echidna-refresh">echidna</a> &ndash; v0.1 (alpha) &mdash;</div>
        </div>
      </div>
    </script>

    <script type="text/ng-template" id="/pdns.html">
      <div class="container-fluid">
        <div class="row-fluid">
          <div class="span12 center" style="text-align: center;">
            <div class="widget-box">
              <div class="widget-title">
                <span class="icon"><i class="icon-exclamation-sign"></i></span>
                <h5>Passive DNS</h5><h6 class="muted">({{dnsCount()}} records)</h6>
              </div>
              <div class="widget-content nopadding">
                <table class="table table-clean">
                  <tr>
                    <th>Query</th>
                    <th>Type</th>
                    <th>Answer</th>
                    <th>TTL</th>
                    <th>Timestamp</th>
                  </tr>
                  <tr ng-repeat="d in dns">
                    <td>{{d.query}}</td>
                    <td>{{d.query_type}}</td>
                    <td>{{d.query_answer}}</td>
                    <td>{{d.ttl}}</td>
                    <td>{{formatTimestamp(d.timestamp)}}</td>
                  </tr>
                </table>
              </div>
            </div>
          </div>
        </div>
        <div class="row-fluid">
          <div id="footer" class="span12">&mdash; <a href="https://github.com/firnsy/echidna-refresh">echidna</a> &ndash; v0.1 (alpha) &mdash;</div>
        </div>
      </div>
    </script>
  </body>
</html>
